    "npm": "use yarn instead",
    "yarn": "4.5.2"
  },
  "packageManager": "yarn@4.5.2",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.node.json",
