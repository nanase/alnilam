    "@vueuse/core": "^11.3.0",
    "happy-dom": "^15.11.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "sass-embedded": "^1.81.0",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2",
    "unplugin-vue-macros": "^2.13.4",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
