    "./object": {
      "types": "./dist/lib/object.d.ts",
      "import": "./dist/lib/object.js"
    },
    "./page": {
      "types": "./dist/lib/page.d.ts"
    },
    "./siPrefix": {
      "types": "./dist/lib/siPrefix.d.ts",
      "import": "./dist/lib/siPrefix.js"
